#include <stdio.h>
#include<math.h>

int main() {
    int popularity,size,price;
    printf("Enter the popularity of the neighborhood: ");
    scanf("%d",&popularity);
    printf("Enter the size of your house; ");
    scanf("%d",&size);
    price = (pow(size,2)+pow(popularity,3))*10000;
    printf("Price of the house is : %d",price);
    return 0;
}
